{
  "name": "online-enrollment-system",
  "version": "1.0.0",
  "description": "A distributed enrollment system with microservices architecture",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run start:auth\" \"npm run start:courses\" \"npm run start:enrollment\" \"npm run start:grades\" \"npm run start:frontend\"",
    "start:frontend": "cd frontend && npm start",
    "start:auth": "cd nodes/auth-node && npm start",
    "start:courses": "cd nodes/course-node && npm start",
    "start:enrollment": "cd nodes/enrollment-node && npm start",
    "start:grades": "cd nodes/grade-node && npm start",
    "seed:professors": "cd nodes/auth-node && node src/db/seed.js",
    "seed:courses": "cd nodes/course-node && node src/db/seed.js",
    "seed:all": "npm run seed:professors && npm run seed:courses"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "mongoose": "^8.13.2"
  }
}
